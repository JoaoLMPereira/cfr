     {
      "name": "postExecution",
      "value": "function f() {\n    \n    var $ph = $('#' + this.htmlObject),\n        $header = $ph.find('thead');\n    \n    $.each( $header.find(\"th.column0\"), function(i,d){ $(d).text(\"User\"); });\n    $.each( $header.find(\"th.column1\"), function(i,d){ $(d).text(\"File\"); });\n    $.each( $header.find(\"th.column2\"), function(i,d){ $(d).text(\"Date\"); });\n    \n    // hook click event on each file row\n    var $pb = $('#' + this.htmlObject),\n        $body = $pb.find('tbody');\n        \n    // selected file css class\n    var selectedCssClass = 'selected';\n    var selectedFileParamName = 'selectedFileParam'\n    \n    $.each( $body.find('tr'), function(index, element) {\n        $(element).click(function() {\n            // remove selected class from any other row\n            $.each( $body.find('tr'), function(i, e) {\n                if (!$(e).is(element)) $(e).removeClass(selectedCssClass);\n            });\n            \n            // toggle state for\n            $(element).toggleClass(selectedCssClass);\n            \n            if ($(element).hasClass(selectedCssClass)) {\n                Dashboards.fireChange(selectedFileParamName, $(element).find('.column0').text());\n            } else {\n                Dashboards.fireChange(selectedFileParamName, \"\");\n            }\n        });\n    });\n}\n\n\n",
      "type": "JavaScript"
     },
     {
      "name": "postFetch",
      "value": "function filterData(data) {\n    \n    if ($.inArray(\"Admin\", Dashboards.context.roles)>-1) {\n        return data;\n    }\n    \n    var array = [];\n    var j = 0;\n    for ( i = 0; i < data.resultset.length; i++ ){\n        \n        if ( Dashboards.context.user == data.resultset[i][1] ){\n            array[j] = data.resultset[i];\n            j++;\n        }\n    }\n    \n    data.queryInfo.totalRows = j;\n    data.resultset = array;\n    return data;\n}",
      "type": "JavaScript"
